  "scripts": {
    "build": "tsc",
    "start": "node js/index.js",
    "dev": "cross-env NODE_ENV=development nodemon js/index.js",
    "debug": "nodemon --inspect js/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
